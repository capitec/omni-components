{
  "name": "@innofake/omni-components",
  "version": "0.0.1",
  "description": "Omni Components come with minimal dependencies to reduce the bloat in your project. All web components base off Lit (https://lit.dev) and are each available as fully standalone imports or at a group level import.",
  "scripts": {
    "clean": "npx rimraf dist docs",
    "lint": "eslint . --ext .ts",
    "build": "npm run compile && npm run docs",
    "compile": "npm run clean && tsc",
    "prepare": "(node -e \"if (! require('fs').existsSync('./dist')) {process.exit(1)} \" || (echo \"Dist does not exist. Attempting Typescript compile\" && tsc && echo \" Made in prepare \" > ./dist/_prepare_gen_ ))",
    "docs": "npx rimraf docs && npm run docs:custom-elements && npm run docs:readme && npm run docs:generate && npm run docs:theme-variables && npm run docs:storybook",
    "docs:generate": "npm run docs:elements && readme generate",
    "docs:custom-elements": "npx cem analyze --litelement --globs src/**",
    "docs:storybook": "npx build-storybook --output-dir=docs",
    "docs:theme-variables": "node stories/theme-variables-parser.js && node stories/themes-list.js",
    "docs:readme": "npm run docs:readme:ui && npm run docs:readme:non-ui",
    "docs:readme:ui": "npx web-component-analyzer ./src/**/*.{js,ts} --outFiles {dir}/README.md",
    "docs:readme:non-ui": "node stories/markdown-gen.js",
    "docs:elements": "node stories/elements-list.js",
    "storybook": "npm run clean && tsc && npm run docs:custom-elements && npm run docs:readme:non-ui && npm run docs:theme-variables && concurrently -k -r \"tsc --build --watch --preserveWatchOutput\" \"npx start-storybook --no-manager-cache --port=6006\"",
    "test:storybook": "concurrently -k -r -s first \"npm run storybook\" \"npx wait-on tcp:6006 && test-storybook --verbose --coverage\"",
    "test:jest": "jest --passWithNoTests --coverage",
    "test": "npm run test:storybook && npm run test:jest"
  },
  "files": [
    "dist",
    "!dist/**/*.stories.*",
    "!dist/**/*.tests.*",
    "!dist/**/*.test.*",
    "src/**/README.md",
    "src/**/*.ts",
    "src/**/*.js",
    "!src/**/*.stories.*",
    "!src/**/*.tests.*",
    "!src/**/*.test.*"
  ],
  "exports": {
    "./*": "./dist/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/innofake/omni-components.git"
  },
  "keywords": [],
  "author": "innofake",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/innofake/omni-components/issues"
  },
  "homepage": "https://github.com/innofake/omni-components#readme",
  "devDependencies": {
    "@appnest/readme": "^1.2.7",
    "@custom-elements-manifest/analyzer": "^0.6.3",
    "@custom-elements-manifest/to-markdown": "^0.1.0",
    "@etchteam/storybook-addon-css-variables-theme": "^1.2.1",
    "@innofake/storybook-addon-cssprops": "^2.3.6",
    "@storybook/addon-a11y": "^6.5.9",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-coverage": "^0.0.2",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addon-storysource": "^6.5.9",
    "@storybook/jest": "^0.0.10",
    "@storybook/test-runner": "^0.5.0",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "^6.5.9",
    "@storybook/web-components": "^6.5.9",
    "@types/jest": "^27.0.0",
    "@types/prismjs": "^1.26.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "babel-jest": "^27.0.0",
    "concurrently": "^7.2.2",
    "eslint": "^8.22.0",
    "eslint-plugin-lit": "^1.6.1",
    "jest": "^27.0.0",
    "jest-github-actions-reporter": "^1.0.3",
    "jest-playwright-preset": "^1.5.1",
    "@innofake/jsdoc-api-debuggable": "^7.1.1",
    "jsdoc-parse": "^6.1.0",
    "pretty": "^2.0.0",
    "react-textarea-autosize": "^8.3.4",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.0",
    "typescript": "^4.7.4",
    "uuid": "8.3.2",
    "wait-on": "^6.0.1",
    "web-component-analyzer": "^1.1.6"
  },
  "dependencies": {
    "lit": "^2.2.7",
    "prismjs": "1.23.0",
    "rxjs": "6.6.6"
  },
  "customElements": "custom-elements.json"
}
